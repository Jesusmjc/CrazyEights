<Window x:Class="CrazyEights.VentanaConfirmación"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CrazyEights"
        xmlns:p = "clr-namespace:CrazyEights.Properties"
        mc:Ignorable="d"
        Title="VentanaConfirmación" Height="230" Width="400">
    <Grid>
        <Image Margin="0,-8,0,-8" Source="/paginas/FondoVentanaEmergente.png" Stretch="Fill"/>
        <Button x:Name="btnAceptar" Content="{x:Static p:Resources.btnAceptar}" HorizontalAlignment="Left" Height="22" Margin="320,146,0,0" VerticalAlignment="Top" Width="57" Click="CerrarVentana" FontSize="10"/>
        <TextBlock x:Name="tbRegistroExitoso" HorizontalAlignment="Center" Height="70" TextWrapping="Wrap" Text="{x:Static p:Resources.tbRegistroExitoso}" VerticalAlignment="Center" Width="296" FontSize="10"/>
        <Label x:Name="lbTituloRegistro" Content="{x:Static p:Resources.lbTituloRegistro}" HorizontalAlignment="Center" Height="27" Margin="0,10,0,0" VerticalAlignment="Top" Width="135" FontWeight="Bold"/>

    </Grid>
</Window>
